{
  "name": "graphql-to-sql",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "prestart": "node preflight.js",
    "start": "export DEBUG=GQL2SQL* && nodemon  --watch src -r ts-node/register --inspect=32211 src/run.ts",
    "test": "jest --watch",
    "docs": "typedoc --exclude ./src/__tests__/**/* --out ./docs ./src",
    "docs:deploy": "yarn docs && cd ./docs && now --name graphql-to-sql --public"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "commander": "^2.19.0",
    "debug": "^4.1.1",
    "inquirer": "^6.2.2",
    "pg": "^7.8.0",
    "prisma-datamodel": "^1.25.3",
    "sequelize": "^4.42.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.1.0",
    "@types/axios": "^0.14.0",
    "@types/debug": "^0.0.31",
    "@types/jest": "^23.3.13",
    "@types/sequelize": "^4.27.34",
    "dotenv": "^6.2.0",
    "jest": "^24.0.0",
    "nodemon": "^1.18.9",
    "now": "^13.1.2",
    "ts-node": "^8.0.2",
    "typedoc": "^0.14.2",
    "typescript": "^3.2.4"
  }
}
